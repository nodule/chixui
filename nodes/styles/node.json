{
  "name": "flowControl",
  "ns": "chixui",
  "description": "Flow Control",
  "phrases": {
    "active": "Adding Flow Control"
  },
  "require": {
    "chix-ui": "0.x.x"
  },
  "ports": {
    "input": {
      "element": {
        "type": "any",
        "title": "Dom Element"
      },
      "event": {
        "type": "string",
        "enum": [
          "select",
          "change",
          "submit",
          "reset",
          "focus",
          "blur"
        ],
        "title": "Dom Event"
      }
    },
    "output": {
      "event": {
        "type": "object",
        "title": "Event"
      }
    }
  }
}
